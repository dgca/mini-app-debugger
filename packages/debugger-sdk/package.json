{
  "name": "@mini-app-debugger/sdk",
  "version": "1.0.0",
  "description": "SDK for debugging mini apps running in Farcaster clients",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist"
  },
  "keywords": ["debugging", "mini-app", "farcaster", "development"],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
